<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.icia.drawAcademy.dao.classdao.ClassDao">
  <insert id="class1proc" parameterType="classDto" >
		INSERT INTO teacherName1
		VALUES (null,#{className},#{day},#{date},#{time},#{m_name}
		)
	</insert>
  	
  	<update id="PlusMemberClassName" parameterType="String">
  		UPDATE membertbl SET className = #{className} WHERE m_name = #{m_name};
  		
  		UPDATE teacherName1 SET m_name = #{m_name} WHERE className = #{className};
  	</update>
  </mapper>